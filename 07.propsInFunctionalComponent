//props in functional components


 function App(){
   return <div>
   <Home name="Cheese" price="99.99"/>
   <Home name="Biryani" price="199.99"/>
   <Home name="Malai Kofta" price="179.99"/>
   <Home name="Coca Cola" price="49.99"/>
   </div>
 
 function Home(props){
   return <div>
     <b>Name:</b> {props.name}<br/>
     <b>Price:</b> {props.price}<br/>
   </div>;
 
 const el = <App/>
 ReactDOM.render(
   el,
   document.getElementById("root")
 
